@model CUATRG.ViewModels.ListImageViewModel

@{
    ViewBag.Title = "ProcessedIndex";
    Layout = "~/Views/Shared/_AdminPartial.cshtml";
}

<div class="container-fluid res-lst">
    <div class="twrp_dv">
        <div class="page-header my_pg_hd">
            <h1>Processed Images<small></small></h1>
        </div>
        <div class="pull-right"><a title="Edit Processed Image" class="my_act_btn" href="#">
                            <button type="button" class="btn btn-primary btn-xs">
                                <span class="glyphicon "></span> + Add New
                            </button>
                        </a></div>
        <div class="clearfix"></div>
        <table class="table table-striped">
            <thead>
            <tr>
                <th>#</th>
                <th>Image</th>
                <th>Master Image</th>
                <th>Filter</th>
                <th>Color Mode</th>
                <th>Album</th>
                <th>Env Condition</th>
                <th>Feature</th>
                <th></th>
            </tr>
            </thead>
             
            @foreach (CUATRG.Models.tblProcessedImage processed in Model.ProcessedImages){ 
                <tr>
                    <td>@processed.PIM_IDPkey</td>
                    <td>@processed.PIM_Name</td>
                    <td>@processed.tblImage.IMG_Name</td>
                    <td>@processed.tblFilter.FLT_Name</td>
                    <td>@processed.tblColorMode.CMD_Name</td>
                    <td>@processed.tblImage.tblAlbum.ALB_Name</td>
                    <td>@processed.tblImage.tblEnvironmentalCondition.ENC_Name</td>
                    <td>@processed.tblImage.tblFeature.FTR_Name</td>
                    <td class="text-right">
                        <a title="Edit Processed Image" class="my_act_btn" href="#">
                            <button type="button" class="btn btn-primary btn-xs">
                                <span class="glyphicon "></span> Edit
                            </button>
                        </a>
                        <a title="Delete Processed Image" class="my_act_btn" href="#">
                            <button type="button" class="btn btn-danger btn-xs">
                                <span class="glyphicon "></span> Delete
                            </button>
                        </a>
                    </td>
                </tr>
            }
           
        </table>
    </div>
</div>


